@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <table>
        <tr>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>First Name</th>
            <th>First Name</th>

        </tr>


        @foreach(Bowler b in ViewBag.Bowlers)
            {
    <tr>
        <td>@b.BowlerFirstName</td>
        <td>@b.BowlerMiddleInit</td>
        <td>@b.BowlerLastName</td>
        <td>@b.BowlerAddress</td>
        <td>@b.BowlerCity</td>
        <td>@b.BowlerState</td>
        <td>@b.BowlerZip</td>
        <td>@b.BowlerPhoneNumber</td>
    </tr>
            }
    </table>


    <div class="d-flex justify-content-center">
        <a>Add Bowler</a>
    </div>
</div>
